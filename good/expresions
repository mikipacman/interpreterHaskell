int n = 10;
int[n] arr; 

int init_arr(int n) {
	for (int i = 0; i < n; i = i + 1) {
		arr[i] = n - i;
	}

	return 0; 
}

// Bubble sort
int sort(int n) {
	int swap;

	for (int i = 0; i < n - 1; i = i + 1) {
    	for (int j = 0; j < n - i - 1; j = j + 1) {
			if (arr[j] > arr[j + 1]) {
				swap = arr[j];
				arr[j] = arr[j + 1];
				arr[j + 1] = swap;
			}
    	}
  	}

  	return 0;
}

int print_arr() {
	for (int i = 0; i < n; i = i + 1) {
		printInt(arr[i]);
	}

	return 0;
}

int main() {

	int arr_reads, arr_writes, comparisons;
	
	init_arr(n);
	printStr("BEFORE");
	print_arr();

	sort(n);
	printStr("AFTER");
	print_arr();


	return 0;
}